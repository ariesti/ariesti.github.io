---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Lovelli Fuad's Blog</title>
	<link href="http://ariesti.github.io/home/atom.xml" rel="self"/>
	<link href="http://ariesti.github.io/home"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://ariesti.github.io/home</id>
	<author>
		<name>@ariesti</name>
		<email>lovelliariesti@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://ariesti.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://ariesti.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
