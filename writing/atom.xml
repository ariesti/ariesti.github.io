---
layout: feed2
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Her Writings</title>
	<link href="http://ariesti.github.io/writing/atom.xml" rel="self"/>
	<link href="http://ariesti.github.io/writing"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://ariesti.github.io/writing</id>
	<author>
		<name>@ariesti</name>
		<email>lovelliariesti@gmail.com</email>
	</author>

	{% for post in site.arts %}
		<entry>
			<title>{{ art.title }}</title>
			<link href="http://ariesti.github.io{{ art.url }}"/>
			<updated>{{ art.date | date_to_xmlschema }}</updated>
			<id>http://ariesti.github.io/{{ art.id }}</id>
			<content type="html">{{ art.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
